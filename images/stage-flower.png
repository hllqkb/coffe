<svg xmlns="http://www.w3.org/2000/svg" width="64" height="64" viewBox="0 0 64 64">
  <!-- 土壤 -->
  <rect x="0" y="50" width="64" height="14" fill="#8B4513"/>
  <rect x="0" y="53" width="64" height="11" fill="#654321"/>
  
  <!-- 主茎 -->
  <rect x="30" y="15" width="4" height="38" fill="#228B22"/>
  
  <!-- 左侧分支 -->
  <rect x="20" y="25" width="12" height="2" fill="#228B22" transform="rotate(-15 26 26)"/>
  <rect x="18" y="35" width="14" height="2" fill="#228B22" transform="rotate(-10 25 36)"/>
  
  <!-- 右侧分支 -->
  <rect x="32" y="25" width="12" height="2" fill="#228B22" transform="rotate(15 38 26)"/>
  <rect x="32" y="35" width="14" height="2" fill="#228B22" transform="rotate(10 39 36)"/>
  
  <!-- 叶子 -->
  <ellipse cx="15" cy="28" rx="5" ry="3" fill="#228B22" transform="rotate(-20 15 28)"/>
  <ellipse cx="13" cy="38" rx="6" ry="3" fill="#32CD32" transform="rotate(-15 13 38)"/>
  <ellipse cx="49" cy="26" rx="5" ry="3" fill="#228B22" transform="rotate(20 49 26)"/>
  <ellipse cx="51" cy="36" rx="6" ry="3" fill="#32CD32" transform="rotate(15 51 36)"/>
  
  <!-- 咖啡花朵 -->
  <!-- 花朵1 -->
  <g transform="translate(25,20)">
    <circle cx="0" cy="0" r="3" fill="white"/>
    <circle cx="-2" cy="-1" r="1.5" fill="white"/>
    <circle cx="2" cy="-1" r="1.5" fill="white"/>
    <circle cx="-1" cy="2" r="1.5" fill="white"/>
    <circle cx="1" cy="2" r="1.5" fill="white"/>
    <circle cx="0" cy="0" r="0.5" fill="#FFD700"/>
  </g>
  
  <!-- 花朵2 -->
  <g transform="translate(39,18)">
    <circle cx="0" cy="0" r="2.5" fill="white"/>
    <circle cx="-1.5" cy="-1" r="1.2" fill="white"/>
    <circle cx="1.5" cy="-1" r="1.2" fill="white"/>
    <circle cx="-1" cy="1.5" r="1.2" fill="white"/>
    <circle cx="1" cy="1.5" r="1.2" fill="white"/>
    <circle cx="0" cy="0" r="0.4" fill="#FFD700"/>
  </g>
  
  <!-- 花朵3 -->
  <g transform="translate(32,12)">
    <circle cx="0" cy="0" r="2" fill="white"/>
    <circle cx="-1.2" cy="-0.8" r="1" fill="white"/>
    <circle cx="1.2" cy="-0.8" r="1" fill="white"/>
    <circle cx="-0.8" cy="1.2" r="1" fill="white"/>
    <circle cx="0.8" cy="1.2" r="1" fill="white"/>
    <circle cx="0" cy="0" r="0.3" fill="#FFD700"/>
  </g>
